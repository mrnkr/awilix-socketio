{
  "name": "awilix-socketio",
  "version": "0.0.0-development",
  "description": "Awilix helpers for Socket.io",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "tslint --project tsconfig.json --fix \"src/**/*.ts\" && prettier --write \"src/**/*.ts\"",
    "cover": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged && npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrnkr/awilix-socketio.git"
  },
  "files": [
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "directories": {
    "lib": "lib"
  },
  "keywords": [],
  "author": "Mr.NKR <xmr.nkr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrnkr/awilix-socketio/issues"
  },
  "homepage": "https://github.com/mrnkr/awilix-socketio#readme",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.15.2",
    "@types/rimraf": "^2.0.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "assert-request": "^1.0.6",
    "awilix": "^4.1.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-latest": "^6.24.1",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "awilix": "^4.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix --project tsconfig.json",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "modulePaths": [
      "src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ],
    "transform": {
      "\\.(ts|tsx)": "ts-jest",
      "\\.(js|jsx)": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {}
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "release": {
    "generateNotes": "@semantic-release/release-notes-generator"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": "transform-decorators-legacy"
  }
}
